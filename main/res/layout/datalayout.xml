<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/teal_200">

    <TextView
        android:id="@+id/header1"
        android:layout_width="158dp"
        android:layout_height="100dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="68dp"
        android:text="Run"
        android:textSize="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/header2"
        android:layout_width="158dp"
        android:layout_height="100dp"
        android:layout_marginTop="68dp"
        android:layout_marginRight="4dp"
        android:text="Lifetime"
        android:textSize="40dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/oncreate1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/header1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/oncreate_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onstart1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/oncreate1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/onstart_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onresume1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/onstart1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/onresume_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onpause1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/onresume1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/onpause_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onstop1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/onpause1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/onstop_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onrestart1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/onstop1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/onrestart_0"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/ondestroy1"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/onrestart1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/ondestroy_0"
        android:textSize="22dp">

    </TextView>


    <TextView
        android:id="@+id/oncreate2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/header2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/oncreate_01"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onstart2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/oncreate2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/onstart_1"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onresume2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/onstart2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/onresume_1"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onpause2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/onresume2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/onpause_1"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onstop2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/onpause2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/onstop_1"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/onrestart2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/onstop2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/onrestart_1"
        android:textSize="22dp">

    </TextView>

    <TextView
        android:id="@+id/ondestroy2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/onrestart2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/ondestroy_1"
        android:textSize="22dp">

    </TextView>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/deleterun"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toBottomOf="@id/ondestroy1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Delete Run"
        android:textSize="22dp"
        android:background="@color/purple_200"
        android:onClick="delrun"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/deletelife"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginRight="4dp"
        app:layout_constraintTop_toBottomOf="@id/ondestroy2"
        app:layout_constraintRight_toRightOf="parent"
        android:text="Delete Lifetime"
        android:textSize="22dp"
        android:background="@color/purple_200"
        android:onClick="dellife"/>


</androidx.constraintlayout.widget.ConstraintLayout>